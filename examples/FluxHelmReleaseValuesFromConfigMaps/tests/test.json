{
      "review": {
        "object": {
          "apiVersion": "helm.toolkit.fluxcd.io/v1",
          "kind": "HelmRelease",
          "metadata": {
            "name": "example-release",
            "namespace": "default"
          },
          "spec": {
            "interval": 60,
            "chart": {
              "spec": {
                "chart": "some-chart",
                "sourceRef": {
                  "kind": "HelmRepository",
                  "name": "some-repo",
                  "namespace": "default"
                }
              }
            },
            "valuesFrom": [
              {
                "configMapKeyRef": {
                  "name": "allowed-configmap"
                }
              },
              {
                "configMapKeyRef": {
                  "name": "disallowed-configmap"
                }
              }
            ]
          }
        }
      },
      "parameters": {
        "exclude_namespaces": [],
        "exclude_label_key": "",
        "exclude_label_value": "",
        "allowed_configmaps": [
          "allowed-configmap"
        ]
      }
    }
    